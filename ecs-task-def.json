{
  "family": "python-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::360477615168:role/ecsTaskExecutionRole1",
  "containerDefinitions": [
    {
      "name": "my-python-app",
      "image": "REPLACE_IMAGE_URI",
      "cpu": 256,
      "memory": 512,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/python-task",
          "awslogs-region": "us-west-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
